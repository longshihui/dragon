{
    "productName": "Dragon",
    "appId": "com.longshihui.Dragon",
    "files": [
        "dist/**/*",
        "public/**/*",
        "package.json"
    ],
    "asar": true,
    "dmg": {
        "background": "./public/dmg-bg.jpg",
        "contents": [
            {
                "x": 130,
                "y": 220
            },
            {
                "x": 410,
                "y": 220,
                "type": "link",
                "path": "/Applications"
            }
        ]
    },
    "win": {
        "target": ["nsis", "msi"]
    },
    "directories": {
        "buildResources": "resources",
        "output": "release"
    },
    "publish": {
        "provider": "github",
        "owner": "electron-react-boilerplate",
        "repo": "electron-react-boilerplate",
        "private": false
    }
}
