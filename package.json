{
  "name": "dragon",
  "version": "1.0.0",
  "description": "一条龙服务",
  "main": "./dist/main.js",
  "scripts": {
    "dev": "cross-env HOT=1 NODE_ENV=development ts-node --project ./cli/tsconfig.json ./cli/scripts/dev.ts",
    "build": "cross-env NODE_ENV=production ts-node --project ./cli/tsconfig.json ./cli/scripts/build.ts",
    "package": "yarn build && electron-builder build --publish never",
    "package-all": "yarn build && electron-builder build -mw",
    "package-win": "yarn build && electron-builder build --win --x64",
    "package-mac": "yarn build && electron-builder build --mac",
    "prestart": "yarn build",
    "start": "cross-env NODE_ENV=production electron ./dist/main.js",
    "prettier": "prettier --write \"./src/**/*.{js,jsx,ts,tsx,scss,css}\"",
    "lint": "eslint --fix './src/**/*.{js,jsx,ts,tsx}'",
    "release": "ts-node --project tsconfig-for-node.json ./scripts/release.ts",
    "create-icon": "electron-icon-builder --input=./public/logo.png --output=./public/app-icon"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/longshihui/dragon.git"
  },
  "author": "longshihui",
  "license": "Apache-2.0",
